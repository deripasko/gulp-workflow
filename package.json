{
  "name": "gulp-workflow",
  "version": "2.0.0",
  "description": "Gulp workflow for front-end development",
  "main": "gulpfile.js",
  "author": {
    "name": "mohamdio",
    "url": "https://mohamd.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohamdio/gulp-workflow"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "1.0.4",
    "bootstrap": "4.0.0",
    "bulma": "0.6.2",
    "foundation-sites": "6.4.4-rc1",
    "jquery": "3.3.1"
  },
  "devDependencies": {
    "browser-sync": "2.23.6",
    "browserify": "16.1.1",
    "del": "3.0.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "5.0.0",
    "gulp-cached": "1.1.1",
    "gulp-changed": "3.2.0",
    "gulp-clean-css": "3.9.3",
    "gulp-concat": "2.6.1",
    "gulp-cssbeautify": "1.0.0",
    "gulp-favicons": "2.2.7",
    "gulp-filter": "5.1.0",
    "gulp-flatten": "0.4.0",
    "gulp-help": "1.6.1",
    "gulp-imagemin": "4.1.0",
    "gulp-inject": "4.3.1",
    "gulp-load-plugins": "1.5.0",
    "gulp-load-subtasks": "1.1.2",
    "gulp-newer": "1.4.0",
    "gulp-nunjucks-render": "2.2.2",
    "gulp-plumber": "1.2.0",
    "gulp-postcss": "7.0.1",
    "gulp-prettify": "0.5.0",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.6.1",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-strip-css-comments": "2.0.0",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "imagemin-pngquant": "5.1.0",
    "lazypipe": "1.0.1",
    "lodash.assign": "4.2.0",
    "main-yarn-files": "3.0.4",
    "npmfiles": "0.1.1",
    "postcss-uncss": "0.16.1",
    "run-sequence": "2.2.1",
    "sassdoc": "2.5.0",
    "uncss": "0.16.2",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0",
    "watchify": "3.11.0"
  },
  "overrides": {
    "jquery": {
      "main": [
        "dist/jquery.js",
        "dist/jquery.min.js"
      ]
    },
    "bootstrap": {
      "main": [
        "scss/**/*.scss",
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.min.js"
      ]
    },
    "bulma": {
      "main": [
        "scss/**/*.scss",
        "css/bulma.css"
      ]
    },
    "foundation-sites": {
      "main": [
        "scss/**/*.scss",
        "dist/css/foundation.css",
        "dist/js/foundation.js",
        "dist/js/foundation.min.js"
      ]
    },
    "@fortawesome/fontawesome-free-webfonts": {
      "main": [
        "scss/**/*.scss",
        "css/fontawesome.css",
        "webfonts/**/*"
      ]
    }
  }
}
